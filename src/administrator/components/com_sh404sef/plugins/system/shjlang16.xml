<?xml version="1.0" encoding="utf-8"?>
<install version="1.5" type="plugin" group="system" method="upgrade">
	<name>J16 Language backport - system - plugin</name>
	<author>Yannick Gaultier</author>
	<creationDate>2010-08-09</creationDate>
	<copyright>2010 - Yannick Gaultier</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail>shumisha@gmail.com</authorEmail>
	<authorUrl>siliana.com</authorUrl>
	<version>1.0.0.545</version>
	<description>Allow using Joomla! 1.6 language file format inside
		Joomla! 1.5.x</description>
	<files>
		<filename plugin="shjlang16">shjlang16.php</filename>
		<folder>shjlang16</folder>
	</files>
	<params>
		<param name="plugin_element" default="shjlang16" type="hidden" />
		<param name="plugin_folder" default="system" type="hidden" />
	</params>
</install>
