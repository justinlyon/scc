	<description>This component rewrites Joomla! URLs to be Search Engine
		Friendly. It has plugins, for handling various components, can work
		without .htaccess, build up meta tags automatically or manually, and
		more. 
</description>
	<installfile>install.sh404sef.php</installfile>
	<uninstallfile>uninstall.sh404sef.php</uninstallfile>
	<install>
		<queries>
			<query>CREATE TABLE IF NOT EXISTS `#__redirection` (
				`id` int(11) NOT
				NULL auto_increment,
				`cpt` int(11) NOT NULL default '0',
				`rank`
				int(11) NOT NULL default '0',
				`oldurl` varchar(255) NOT NULL default
				'',
				`newurl` varchar(255) NOT NULL default '',
				`dateadd` date NOT NULL
				default '0000-00-00',
				PRIMARY KEY (`id`),
				KEY `newurl` (`newurl`),
				KEY
				`rank` (`rank`),
				KEY `oldurl` (`oldurl`)
				) TYPE=MyISAM DEFAULT CHARACTER SET `utf8`;
			</query>
			<query>CREATE TABLE IF NOT EXISTS `#__sh404SEF_meta` (
				`id` int(11)
				NOT NULL auto_increment,
				`newurl` varchar(255) NOT NULL default '',
				`metadesc` varchar(255) default '',
				`metakey` varchar(255) default
				'',
				`metatitle` varchar(255) default '',
				`metalang` varchar(30)
				default '',
				`metarobots` varchar(30) default '',
				PRIMARY KEY (`id`),
				KEY `newurl` (`newurl`)
				) TYPE=MyISAM DEFAULT CHARACTER SET `utf8`;
			</query>
			<query>CREATE TABLE IF NOT EXISTS `#__sh404sef_aliases` (
				`id` int(11) NOT NULL auto_increment,
				`newurl` varchar(255) NOT NULL default '',
				`alias` varchar(255) NOT NULL default '',
				`type` tinyint(3) NOT NULL DEFAULT '0',
        `hits` int(11) NOT NULL DEFAULT '0',
				PRIMARY KEY (`id`),
				KEY	`newurl` (`newurl`),
				KEY `alias` (`alias`),
        KEY `type` (`type`)
				) TYPE=MyISAM DEFAULT CHARACTER SET `utf8`;
			</query>
			<query>CREATE TABLE IF NOT EXISTS `#__sh404sef_pageids` (
        `id` int(11) NOT NULL auto_increment,
        `newurl` varchar(255) NOT NULL default '',
        `pageid` varchar(255) NOT NULL default '',
        `type` tinyint(3) NOT NULL DEFAULT '0',
        `hits` int(11) NOT NULL DEFAULT '0',
        PRIMARY KEY (`id`),
        KEY `newurl` (`newurl`),
        KEY `alias` (`pageid`),
        KEY `type` (`type`)
        ) TYPE=MyISAM DEFAULT CHARACTER SET `utf8`;
      </query>
		</queries>
	</install>
