			<option value="1">JSHOW</option>
		</field>

		<field name="show_telephone_headings"
			type="radio"
			default="1" label="COM_CONTACT_FIELD_CONFIG_PHONE_LABEL"
			description="COM_CONTACT_FIELD_CONFIG_PHONE_DESC"
		>
			<option value="0">JHIDE</option>
			<option value="1">JSHOW</option>
